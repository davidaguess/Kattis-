using System;

namespace DavidGuessStuckInATimeLoop
{
    class Program
    {
        static void Main(string[] args)
        {
            try 
            {
                int N = int.Parse(Console.ReadLine());
                int count = 1;

                if (N >= 1 && N <= 100)
                {
                    while (count <= N)
                    {
                        Console.Write(count + " Abracadabra");
                        Console.WriteLine();
                        count = count + 1;
                    }
                }
                else
                {
                    Console.Write("integer must be between 1 and 100");
                }
            }
            catch
            {
                Console.Write("integer must be between 1 and 100");
            }
        }
    }
}
